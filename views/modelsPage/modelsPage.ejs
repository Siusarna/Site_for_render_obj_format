<!DOCTYPE html>
<html lang="zxx">

<head>
  <meta charset="UTF-8" />
  <link href="http://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet" />
  <link rel="stylesheet" type="text/css" href="/css/modelPage.css" />
  <title>Siusarna-site</title>
</head>

<body>
  <div id="status"></div>
  <h1>My models</h1>
  <form enctype="multipart/form-data" name="addNewModel" >
    <input type="file" name="file-to-upload" id="file" class="inputfile" />
    <label for="file"><strong>Choose a file</strong></label>
  </form>
  <div class="buttons">
    <button class="butSave" type="submit" id="upload">Save</button>
    <button class="butDelet" disabled='true' type="submit" id="delete">Delet model</button>
    <button class="butRender" disabled='true' type="submit" id="render">Render model</button>
  </div>
  <table id="tableWithModels" class="table">
    <tr>
      <td class="col1">Status</td>
      <td class="col2">Id</td>
      <td class="col3">Model name</td>
    </tr>
    <% for(var i=0; i < data.length; i++) { %>
       <tr>
         <td class="col1"><input type="radio" name="select"></td>
         <td class="col2"><%= i+1 %></td>
         <td class="col3"><%= data[i].nameOfModel %></td>
       </tr>
    <% } %>
</table>
<script src="/js/modelPage.js"></script>
</body>

</html>
